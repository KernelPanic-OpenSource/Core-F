function hoanthanh($dem, $type, $id, $xujob, $mau, $xu) {
    
	$a = "\033[1;93m[\033[1;93m".$dem."\033[1;93m]\033[1;91m  ‚óè \033[1;96m".date("H:i")."\033[1;91m ‚óè ".$mau."$type\033[1;91m ‚óè \033[1;97m".$id."\033[1;91m ‚óè\033[1;92m ".$xujob." \033[1;91m‚óè \033[1;92m".$xu."\e[0m\n";
    $len = strlen($a);
    for ($x = 0; $x < $len; $x++) {
        echo $a[$x];
        usleep(1000);
    }
}

